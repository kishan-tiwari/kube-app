{
    "family": "kube-app-demo",
    "executionRoleArn": "arn:aws:iam::728291931126:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "cpu": "256",
    "memory": "512",
    "requiresCompatibilities": [
      "FARGATE"
    ],
    "containerDefinitions": [
        {
            "name": "kube-app",
            "image": "728291931126.dkr.ecr.us-east-1.amazonaws.com/kube-app:8059619-v15",
            "portMappings": [
                {
                    "containerPort": 4000,
                    "hostPort": 4000,
                    "protocol": "tcp"
                }
            ],
            "environment": [
                {
                    "name": "ENV",
                    "value": "Prod"
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-create-group": "true",
                    "awslogs-group": "/ecs/kube-app-demo",
                    "awslogs-region": "us-east-1",
                    "awslogs-stream-prefix": "ecs"
                }
            }
        }
    ]
}

